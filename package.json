{
  "name": "react-d3-speedometer",
  "version": "1.0.2",
  "description": "React library for showing speedometer like gauge using d3.",
  "author": {
    "name": "palerdot",
    "email": "palerdot@gmail.com"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/react-d3-speedometer.umd.js",
  "module": "./dist/react-d3-speedometer.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-d3-speedometer.es.js",
      "require": "./dist/react-d3-speedometer.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/palerdot/react-d3-speedometer.git"
  },
  "bugs": {
    "url": "https://github.com/palerdot/react-d3-speedometer/issues"
  },
  "homepage": "https://github.com/palerdot/react-d3-speedometer",
  "keywords": [
    "react",
    "d3",
    "speedometer",
    "gauge",
    "component",
    "odometer"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "prettier -l **/src/**/*.js",
    "lintfix": "prettier --write **/src/**/*.js",
    "testonly": "vitest",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook --port 6006",
    "publish-storybook": "bash .scripts/publish_storybook.sh",
    "dev": "vite",
    "build": "npm run build:main && npm run build:slim && npm run copy:types",
    "build:main": "NODE_ENV=production vite build",
    "build:slim": "NODE_ENV=production vite build -c slim.config.js",
    "copy:types": "cp ./src/index.d.ts ./dist/",
    "preview": "vite preview",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-docs": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-info": "^6.0.0-alpha.2",
    "@storybook/addon-interactions": "^6.4.21",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/addon-storysource": "^6.4.21",
    "@storybook/builder-vite": "^0.1.26",
    "@storybook/react": "^6.4.21",
    "@storybook/testing-library": "^0.0.9",
    "@storybook/theming": "^6.4.21",
    "@vitejs/plugin-react": "^1.3.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.15.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "git-url-parse": "^11.1.2",
    "happy-dom": "^2.55.0",
    "jest": "^26.5.0",
    "jsdom": "^14.0.0",
    "prettier": "^2.1.2",
    "react": "^18.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^18.0.0",
    "react-test-renderer": "^16.8.4",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "vite": "^2.9.1",
    "vitest": "^0.8.4",
    "webpack": "^5.72.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "d3-array": "^3.1.4",
    "d3-color": "^3.1.0",
    "d3-ease": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "d3-transition": "^3.0.1",
    "lodash-es": "^4.17.21",
    "memoize-one": "^6.0.0",
    "prop-types": "^15.8.1"
  },
  "engines": {
    "node": ">=14.0",
    "npm": ">=6.0.0"
  },
  "sideEffects": false
}
